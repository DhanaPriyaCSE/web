<script>
    AOS.init();
    $(document).ready(()=>{
        var timer = 2000;
        $('.collapsible').collapsible();
        $('select').formSelect();
        $('.tabs').tabs();
        $('.sidenav').sidenav();
        if( document.querySelector('.carousel.carousel-slider')){
            $('.carousel.carousel-slider').carousel({
                interval: 5000,
                indicators: true,
                fullWidth: true
            }).height(() => {
                if (screen.width < 670) {
                    return screen.height - Math.floor(screen.height / 2);
                }
                else if (screen.width > 992 && screen.width < 1021) {
                    return screen.height - Math.floor(screen.height / 2 + 100);
                }
                else {
                    return screen.height - Math.floor(screen.height / 3);
                }
            });
            $(window).resize(function(){
                $('.carousel.carousel-slider').carousel({
                    interval: 5000,
                    indicators: true,
                    fullWidth: true
                }).height(() => {
                    if (screen.width < 670) {
                        return screen.height - Math.floor(screen.height / 2);
                    }
                    else if (screen.width > 992 && screen.width < 1021) {
                        return screen.height - Math.floor(screen.height / 2 + 100);
                    }
                    else {
                        return screen.height - Math.floor(screen.height / 3);
                    }
                });
            });
            
            setInterval(()=>{
                var ele = document.querySelector('#main-carousel');
                var cor =  M.Carousel.getInstance(ele);
                cor.next();
            },5000);

        }
    });
   
        
    
</script>